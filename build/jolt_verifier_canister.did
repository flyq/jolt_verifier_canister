type Error = variant {
  Internal : text;
  NotAuthorized;
  CallError : text;
  StableError : text;
};
type InitData = record { owner : principal };
type Result = variant { Ok; Err : Error };
type Result_1 = variant { Ok : bool; Err : Error };
service : (InitData) -> {
  get_buffer : (nat8) -> (opt blob) query;
  get_owner : () -> (principal) query;
  preprocessing : () -> (Result);
  set_owner : (principal) -> (Result);
  upload_preprocessing_buffer : (nat8, blob) -> (Result);
  verify_jolt_proof : (blob) -> (Result_1) query;
}
